#. type: Bullet: 'o'
#: ../E/cheats.txt:53
#, no-wrap
msgid "<code>getresearchdone </code>Returns done researches bitmask"
msgstr "<code>getresearchdone </code>Visszaadja a kész kutatások bitmaszkját"

#. type: Bullet: 'o'
#: ../E/cheats.txt:54
#, no-wrap
msgid "<code>playmusic    </code>Plays music, <code>1</code>: file name, <code>2</code>: repeat"
msgstr "<code>playmusic    </code>Elindítja a zenét, <code>1</code>: fájl név, <code>2</code>: ismétlés"

#. type: Bullet: 'o'
#: ../E/cheats.txt:55
