msgstr "<code>winmission   </code>Azonnal megnyeri az aktuális küldetést"

#. type: Bullet: 'o'
#: ../E/cheats.txt:9
#, no-wrap
msgid "<code>lostmission  </code>Immediately lose the current mission"
msgstr "<code>lostmission  </code>Azonnal elveszíti az aktuális küldetést"

#. type: Bullet: 'o'
#: ../E/cheats.txt:14
#, no-wrap
msgid "<code>allresearch  </code>All researches are done"
msgstr "<code>allresearch  </code>Minden kutatást készre állít"

#. type: Bullet: 'o'
#: ../E/cheats.txt:15
#, no-wrap
msgid "<code>allbuildings </code>All buildings are enabled"
msgstr "<code>allbuildings </code>Minden épületet engedélyez"

#. type: Bullet: 'o'
#: ../E/cheats.txt:16
#, no-wrap
